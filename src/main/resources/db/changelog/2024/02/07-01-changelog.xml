<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">

    <changeSet id="1744301580-1" author="javierfn">
        <createSequence incrementBy="1" sequenceName="PRODUCT_ID_SEQ" startValue="21"/>
        <rollback>
            <dropSequence sequenceName="PRODUCT_ID_SEQ"/>
        </rollback>
    </changeSet>

    <changeSet id="1744301580-2" author="javierfn">
        <createTable tableName="PRODUCT" tablespace="DATA">
            <column name="ID" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PRODUCT_PK" primaryKeyTablespace="INDEX" notNullConstraintName="PRODUCT_ID_NN"/>
            </column>
            <column name="NAME" type="VARCHAR(100)">
                <constraints nullable="false" notNullConstraintName="PRODUCT_NAME_NN"/>
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="PRODUCT"/>
        </rollback>
    </changeSet>

</databaseChangeLog>
